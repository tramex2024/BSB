{
  "version": 2,
  "builds": [
    {
      "src": "BSB/server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "BSB/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/BSB/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/BSB/public/index.html"
    }
  ]
}